<h3> parts List </h3>
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">model</th>
      <th scope="col">brand</th>
      <th scope="col">price</th>
      <th scope="col">image</th>
    </tr>
  </thead>
  <tbody>
    <% cars.forEach((car)=>{%>
    <tr>
      <td>
        <%=car.c_id%>
      </td>
      <td>
        <%=car.model%>
      </td>
      <td>
        <%=car.brand%>
      </td>
      <td>
        <%=car.price%>
      </td>



      <td>
        <a class="btn btn-primary" href="/cars/edit/<%=car.c_id%>">Edit</a>
<a class="btn btn-danger" href="/cars/delete/<%=car.c_id%>">Delete</a>
      </td>
    </tr>
    <%})%>
  </tbody>
</table>